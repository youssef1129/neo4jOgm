{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@app/neo4jorm/(.*)": "<rootDir>/../libs/neo4jorm/src/$1",
    "@app/neo4jorm": "<rootDir>/../libs/neo4jorm/src"
  }
}